#!/usr/bin/env python
# -*- coding: utf-8 -*-

import pyHIARD.main as main
import sys

#The main is required here when multiprocessing
if __name__ == '__main__':
    from viztracer import VizTracer
    with VizTracer(output_file="Full_Run.json",min_duration=500) as tracer:
        main.main(sys.argv[1:])
